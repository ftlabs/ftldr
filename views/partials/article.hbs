<table>
  <tbody>
    <tr>
      <th colspan="2" style="text-align:center;">Essences</th>
    </tr>
{{#each phrases}}
    <tr>
      <th><strong>{{type}}</strong></th><td>{{text}}</td>
    </tr>
{{/each}}
    <tr>
      <th colspan="2" style="text-align:center;">Context</th>
    </tr>
{{#each context}}
    <tr>
      <th><strong>{{type}}</strong></th><td>{{text}}</td>
    </tr>
{{/each}}
    <tr>
      <th colspan="2" style="text-align:center;">Details</th>
    </tr>
    <tr>
      <th><strong>UUID</strong></th><td>{{uuid}}</td></tr>
      <th><strong>URL</strong></th><td><a href="{{url}}">{{url}}</a></td></tr>
  </tbody>
</table>

<h1><a href="{{url}}">{{{ content.title }}}</a></h1>
<p>{{{ bodyXML }}}</p>
